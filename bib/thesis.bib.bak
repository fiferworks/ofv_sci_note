@Article{Bronner1991,
  author    = {Bronner, R. and Westphal, E. and Dreger, F.},
  journal   = {Canadian Journal of Botany},
  title     = {Enhanced peroxidase activity associated with the hypersensitive response of {\textit{Solanum dulcamara}} to the gall mite {\textit{Aceria cladophthirus}} ({Acari}: {Eriophyoidea})},
  year      = {1991},
  month     = oct,
  number    = {10},
  pages     = {2192--2196},
  volume    = {69},
  abstract  = {Peroxidase activity and its electrophoretic pattern were studied in resistant Solanum dulcamara leaves following infestation by the gall mite Aceria cladophthirus. Total peroxidase activity increased in infested leaves and was related to enhancement of three major peroxidase isozymes. The highest activity was found for the intercellular peroxidases. The extraction procedures used suggest that these peroxidases are pathogenesis-related proteins. These results are discussed with reference to the lack of lignin barrier around the mite-induced necrotic local lesions. Key words: Aceria cladophthirus, gall mite, Solanum dulcamara, resistant plant, pathogenesis-related proteins, peroxidases isozymes.},
  doi       = {10.1139/b91-275},
  publisher = {Canadian Science Publishing},
}

@Article{Bronner1991a,
  author   = {Bronner, R. and Westphal, E. and Dreger, F.},
  journal  = {Physiological and molecular plant pathology},
  title    = {Pathogenesis-related proteins in {\textit{Solanum dulcamara}} {L.} resistant to the gall mite {\textit{Aceria cladophthirus}} ({Nalepa})(syn {\textit{Eriophyes cladophthirus}} {Nal.})},
  year     = {1991},
  abstract = {Feeding by the gall mite Aceria cladophthirus on resistant varieties of Solanum dulcamara leads to the development of small necrotic local lesions. This hypersensitive response in infested leaves is accompanied by the accumulation of several novel proteins referred to as pathogenesis-related proteins (PR proteins). This report expands the range of stimuli triggering PR protein synthesis and confirms that these proteins are markers of the hypersensitive response of plants. Simultaneously, chitinase and beta-1,3-glucanase activities are induced during the development of A. cladophthirus-induced lesions. This suggests that some PR proteins of S. dulcamara have chitinase and beta-1,3-glucanase activities. These enzymes may contribute to the defense responses of plants against mites.},
}

