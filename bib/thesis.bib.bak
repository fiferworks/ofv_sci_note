@Article{PalomaresPerez2020,
  author    = {Palomares-P{\'{e}}rez, Mart{\'{\i}}n and Contreras-Berm{\'{u}}dez, Yadira and Grifaldo-Alc{\'{a}}ntara, Fabi{\'{a}}n and Garc{\'{i}}a-Garc{\'{i}}a, Rosa Elia and Bravo-N{\'{u}}{\~{n}}ez, Manuel and Arredondo-Bernal, Hugo Cesar},
  journal   = {International Journal of Tropical Insect Science},
  title     = {Development of {\textit{Ceraeochrysa cincta}} ({Neuroptera}: {Chrysopidae}) fed with {\textit{Raoiella indica}} ({Acari}: {Tenuipalpidae})},
  year      = {2020},
  month     = oct,
  number    = {2},
  pages     = {1169--1176},
  volume    = {41},
  abstract  = {Given the scarce knowledge available on the biological aspects of the family Chrysopidae fed with phytophagous mites, the present work aims to study the life cycle of the generalist predator, Ceraeochrysa cincta (Schneider) fed with Raoiella indica Hirst, in order to obtain information that will be useful in the integrated management of this pest, which can damage several species of palms. Only 6.4% of the F0 generation fed only R. indica reaches the adult stage. Adults originating from the F0 generation did not exceed the preoviposition period. Adding Sitotroga cerealella (Olivier) eggs to the diet, the F2 generation presented the following results: 23% reached the adult stage, the embryonic period was 6.5\hspace{0.167em}$\pm$\hspace{0.167em}0.5 d. The longest instar (9.3\hspace{0.167em}$\pm$\hspace{0.167em}2.1 d) was 3\textsuperscript{rd} instar. The adult had an average longevity of 81.7\hspace{0.167em}$\pm$\hspace{0.167em}54.9 d. Statistically, there was no significant difference between the survival of females and males (P\hspace{0.167em}=\hspace{0.167em}0.351\hspace{0.167em}>\hspace{0.167em}0.05). The preoviposition, oviposition and postoviposition periods were 6.8\hspace{0.167em}$\pm$\hspace{0.167em}0.9, 58.92\hspace{0.167em}$\pm$\hspace{0.167em}8.31 and, 54.85\hspace{0.167em}$\pm$\hspace{0.167em}10.86 d respectively. The average oviposition rate was 2.04\hspace{0.167em}$\pm$\hspace{0.167em}0.91 eggs per day per female. Population statistics Ro\hspace{0.167em}=\hspace{0.167em}0.5, T\hspace{0.167em}=\hspace{0.167em}128.64 d, rm.\hspace{0.167em}=\hspace{0.167em}0.005 and $\lambda$\hspace{0.167em}=\hspace{0.167em}1.005 indicated poor development of C. cincta using R. indica as primary dam. According to the information obtained, it is concluded that R. indica does not satisfy the nutritional requirements of C. cincta, and as a consequence, needs another food source to complete its biological cycle.},
  doi       = {10.1007/s42690-020-00302-9},
  publisher = {Springer Science and Business Media {LLC}},
}

