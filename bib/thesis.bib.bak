@Article{Childers1994,
  author   = {Carl C. Childers},
  journal  = {Florida Entomologist},
  title    = {Feeding injury to {'Robinson'} tangerine leaves by {\textit{Brevipalpus}} mites ({Acari}: {Tenuipalpidae}) in {Florida} and evaluation of chemical control on citrus},
  year     = {1994},
  month    = jun,
  number   = {2},
  pages    = {265--271},
  volume   = {77},
  abstract = {Larvae, nymphs and adults of the false spider mites Brevipalpus phoenicis (Geijskes) and B. obovatus Donnadieu (Acari: Tenuipalpidae) were found feeding on the ventral surface along the midrib of {\textquoteleft}Robinson{\textquoteright} tangerine leaves during December 1990. Resinous irregular areas were present in association with the chlorotic midvein areas of injured cells on the lower leaf surface. Similar injury was occasionally evident on some lateral veins in association with the mites. Yellowed, blistered areas were evident on the upper leaf surface opposite the injured areas on the lower leaf. Leaf drop was evident only on trees that were heavily infested with false spider mites. This type of leaf injury by Brevipalpus mites has not been previously reported on Florida citrus. Chemical control evaluations showed that AC 303,630 in combination with petroleum oil, pyridaben, fenbutatin-oxide, dicofol or high rates of sulfur provided at least 35 days control. Ethion was less effective and carbaryl failed to control these mites.},
}

@Article{Childers2003c,
  author    = {Childers, Carl C. and Derrick, Kenneth S.},
  journal   = {Experimental and Applied Acarology},
  title     = {{\textit{Brevipalpus}} mites as vectors of unassigned {Rhabdoviruses} in various crops},
  year      = {2003},
  number    = {1-3},
  pages     = {1--3},
  volume    = {30},
  abstract  = {For some time, a limited number of species within the superfamily Eriophyoidea have been the only known acari involved in vectoring viruses to plants (Keifer, 1952; Slykhuis, 1963; Walkey, 1991; Oldfield and Proeseler, 1996). More recently, other acarine families have been incriminated as vectors of plant viruses including Brevipalpus species within the Tenuipalpidae and Petrobia latens (Muller) in the Tetranychidae. The spider mite vectors Barley yellow streak mosaic virus (Robertson, 1987; Robertson and Carroll, 1988; Brunt et al., 1996). This is the only documented instance of a spider mite species involved in the transmission of a viral pathogen. Species within the genus Brevipalpus are considered to be the most important economic pests within the family Tenuipalpidae, especially B. californiens (Banks), B. obovatus Donnadieu and B. phoenicis (Geijskes). All three of these species have been incriminated as vectors of at least one unassigned rhabdovirus. The recent establishment of citrus leprosis virus in Panama and its possible spread to other countries within Central America have increased awareness of the potential threat to citrus in North America and elsewhere in the world. Our limited understanding of Brevipalpus mite vectors, their biologies, feeding damage and virus movement within the mite vectors exacerbate the development of effective control strategies for citrus leprosis. These factors and an increasing list of other unassigned rhabdoviruses that are vectored by Brevipalpus mites has led to the preparation of this special issue to provide a concise update on their status. In the opening paper, Childers, French and Rodrigues provide a review of the biology, feeding injury and economic importance of four Brevipalpus species on selected plants of horticultural importance. Extensive color photographs are provided to clearly illustrate the types of feeding injury associated with these mites. The second paper by Childers, Rodrigues and Welbourn reports on the extensive host plant lists of B. califomicus, B. obovatus, and B. phoenicis. Host plants are reported for seven categories that include: each mite species alone, each two species combination of mites, and host plants that are reportedly shared by all three Brevipalpus species. Welbourn, Ochoa, Kane, and Erbe provide a taxonomic review of the genus Brevipalpus with emphasis on B. phoenicis. The taxonomic characters currently used to separate these three species are included and supported with scanning electron micrographs to facilitate character recognition. The extensive host ranges and variation in taxonomic characters within populations of B. californiens and B. phoenicis raise concerns about possible hidden species complexes occurring within these two species.  Kitajima, Chagas, and Rodrigues provide a review of Brevipalpustransmitted unassigned rhabdoviruses and virus-like diseases. The unassigned rhabdoviruses are classified as either the cytoplasmic type or the nuclear type of virus. Transmission electron microscopy provides clear examples of both types of viruses. In addition, various associated disease symptoms are provided in both black and white and color plates. Rodrigues, Kitajima, Childers, and Chagas present a review of citrus leprosis virus that is vectored by B. phoenicis on citrus varieties in Brazil. Color plates are provided to clearly illustrate symptomatic differences among various sweet orange varieties on the leaves, twigs and fruit. This is the most serious viral disease vectored by Brevipalpus mites and a color plate shows the cumulative differences between an acaricide-treated sweet orange tree and an untreated sweet orange tree left unprotected against citrus leprosis after only two seasons of growth. Childers, Rodrigues, Derrick, Achor, French, Welbourn, Ochoa, and Kitajima report on the past and present status of citrus leprosis in Florida and Texas. Evidence is provided that strongly suggests that citrus leprosis once occurred in Florida counties and then disappeared. Results of an extensive survey of sweet orange trees across Florida counties that once reported the disease have failed to located disease symptoms during 2001 and 2002. Brevipalpus feeding injuries to both sweet orange and grapefruit in Texas are reported. Citrus leprosis does not result in symptom expression on fruit, leaves or twigs of grapefruit varieties. Chagas, Kitajima, and Rodrigues report on the transmission of coffee ringspot virus by B. phoenicis in Brazil. This disease was not an economic problem initially, however, that changed in recent years with evidence of increasing damage to coffee by the disease. Kondo, Maeda, and Tamada report on orchid fleck virus and its vector, B. californicus and the final paper by Kitajima, Rezende, and Rodrigues provides a review of research conducted on Passion fruit green spot that is vectored by B. phoenicis in Brazil.  Some plant feeding insects and mites are effective vectors of plant viruses, in part, because of their high rates of reproduction, dispersal abilities, and obligate use of specific plants as food (Gilbertson et al., 1998). One example is citrus leprosis that is an unassigned rhabdovirus. The virus is not known to exist in citrus within the United States including Hawaii, Europe, South Africa, Australia or different areas within Asia. However, the potential mite vectors in the genus Brevipalpus occur in those countries. Risk for establishment of the virus into one or more of these citrus growing areas must be considered with increasing international movement of people and commodities. In addition, other related and unassigned rhabdoviruses are vectored by Brevipalpus species to coffee, passion fruit, orchids and a wide array of woody ornamentals that are reported in this special issue. The accompanying papers describe the current understanding of the biology, host plants, and distribution of Brevipalpus species as well as their feeding damage and disease symptoms on fruits, leaves and twigs of affected plants.},
  doi       = {10.1023/b:appa.0000006542.96404.63},
  publisher = {Springer Science and Business Media {LLC}},
}

