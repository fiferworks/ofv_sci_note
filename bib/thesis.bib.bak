@Article{Carrillo2011a,
  author    = {Carrillo, Daniel and Frank, J. Howard and Rodrigues, Jose Carlos V. and Pe{\~{n}}a, Jorge E.},
  journal   = {Experimental and Applied Acarology},
  title     = {A review of the natural enemies of the red palm mite, {\textit{Raoiella indica}} ({Acari}: {Tenuipalpidae})},
  year      = {2011},
  month     = oct,
  number    = {3-4},
  pages     = {347--360},
  volume    = {57},
  comment   = {A review of all the available information about the natural enemies reported in association with the red palm mite, Raoiella indica is presented. Twenty-eight species of predatory arthropods, including mites and insects, have been reported in association with R. indica in Asia, Africa and the Neotropics. In addition, pathogenic fungi associated with R. indica in the Caribbean have been reported. The available literature indicates that each site has a different natural enemy complex with only one predator species, Amblyseius largoensis (Acari: Phytoseiidae), present in all the geographical areas. The phytoseiids, Amblyseius caudatus Berlese, Amblyseius channabasavanni Gupta and A. largoensis, were regarded as important natural enemies of R. indica, and their predatory efficiency was studied in some detail. Among the predatory insects the coccinellids Stethorus keralicus Kapur and Telsimia ephippiger Chapin were reported as major predators of R. indica. The known distribution, abundance and relative importance of each species reported in association with R. indica are discussed.},
  doi       = {10.1007/s10493-011-9499-4},
  publisher = {Springer Science and Business Media {LLC}},
}

@Article{Carrillo2011b,
  author    = {Carrillo, Daniel and Navia, Denise and Ferragut, Francisco and Pe{\~{n}}a, Jorge E.},
