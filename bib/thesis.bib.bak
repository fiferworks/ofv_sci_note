  author    = {Ward, Eric R. and Payne, George B. and Moyer, Mary B. and Williams, Shericca C. and Dincher, Sandra S. and Sharkey, Kevin C. and Beck, James J. and Taylor, Hope T. and Ahl-Goy, Patricia and Meins, Frederick and Ryals, John A.},
